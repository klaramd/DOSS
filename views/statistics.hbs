<link rel="stylesheet" type="text/css" href='/stylesheets/statistics.css'>
<div class="tab">
    <button class="tablinks"> <a href="/useraccount"> Account </a> </button>
    <button class="tablinks"> <a href="/community"> Community  </a> </button>

</div>
<h1> Look at your progress</h1>
<div class="training">
    <h2>Your training progress</h2>

    <ul>
        {{#each trainingstest}}
            <li> <div class = "description">{{description}} </div>  on {{startDate}} <br> at {{startTime}} o'clock</li>
        {{/each}}
    </ul>

</div>

<div class="weight">
    <h2>Your weight progress</h2>

    <ul>
        {{#each weighttest}}
            <li><div class = "description">{{weightKG}} kg </div> on  {{weightDate}}</li>
        {{/each}}
    </ul>

</div>

